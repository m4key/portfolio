section.section.primary#parallax
  .parallax
    .parallax__layer(data-speed="0.08" data-parallax-disallow="y" data-parallax-deep="6000")
      +image('content/parallax_one.png', 'parallax__image')
    .parallax__layer(data-speed="0.13" data-parallax-deep="5000")
      +image('content/parallax_two.png', 'parallax__image')
    .parallax__layer(data-speed="0.15" data-parallax-deep="4000")
      +image('content/parallax_three.png', 'parallax__image')
    .parallax__layer(data-speed="0.2" data-parallax-deep="3000")
      +image('content/parallax_four.png', 'parallax__image')
  .container
    .header__wrap
      include header
    .portfolio
      h2.portfolio__title
        | Макейкин Александр
      h3.portfolio__subtitle
        | Личный сайт
        br 
        | веб разработчика
    .scroll
      .scroll__text Скролл
      button.scroll__button
        +icon('scroll', 'scroll__button-svg') 