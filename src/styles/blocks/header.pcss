.header__wrap{
  z-index: 10;
  margin-top: 64px;
}
.header {
  display: flex;
  align-items:center;
}
.header__logo{
  z-index: 70;
}
.header__nav{
  margin-left: 19%;
  @include desktop{
    margin-left: 8%;
  }
  @include tablets{
    display: none;
  }
}

.header__social{
  margin-left: 16%;
  @include desktop{
    margin-left: 8%;
  }
  @include tablets{
    margin-left: 53%;
  }
  @include phones{
    display: none;
  }
}
