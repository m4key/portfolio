.scroll{
  display: flex;
  margin: 0 auto;
  margin-top: 350px;
  cursor: pointer;
}
.scroll__button{
  width: 19px;
  height: 30px;
  background-color: transparent;
  border: 0;
  outline: 0;
  
}
.scroll__text{
  font-size: 18px;
  color:#fff;
  font-weight: 600;
  line-height: 1.67;
}

.scroll__button-svg{
  height: 100%;
  fill:#fff;
}